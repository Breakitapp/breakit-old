html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
  script(type='text/javascript', src='http://ajax.googleapis.com/ajax/libs/jquery/1.5/jquery.min.js')
  script(scr="javascripts/jquery.colorbox.js", type="text/javascript")
  script(type='text/javascript')
    $.ajaxSetup ({
          cache: false
        });
    $(document).ready(function() {
      $('.vote').click(function() {
        var this_ = $(this);
        $.post('/',
         {_id : $(this).attr('data'), points: $(this).attr('points')},
         function(score) {
           $(this_).siblings('.points').text(score.score);
        });
      });
      $('a.popUp').colorBox({html: "<p>moikkelis</p>"});
    });
  
  body!= body

