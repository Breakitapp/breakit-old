a
  color: #00B7FF

.stream
  max-width: 290px
  margin-left: auto
  margin-right: auto
  display:block

#nav-top-line
  height: 18px
  background: -webkit-linear-gradient(top, #8f1b1b, #d03636)
  background: -moz-linear-gradient(top, #8f1b1b, #d03636)

#nav-bottom-line
  min-height: 58px
  background: -webkit-linear-gradient(top, #fff, #ededed)
  background: -moz-linear-gradient(top, #fff, #ededed)
  box-shadow: 0 6px 0 #d1d1d1
  -moz-box-shadow: 0 6px 0 #d1d1d1
  -webkit-box-shadow: 0 6px 0 #d1d1d1

#nav-logo
  margin-left: 60px
  max-height: 58px
  float: left

.navbutton
  height: 58px
  float: left
  border: 1px solid red

.post
  width: 300px
  position: relative
  background: #fff
  border: 1px solid #c8c8c8
  margin: 16px
  float: left
  display: inline
  margin: 0 5px 5px 8px
   
  

#footer
 clear: both
 height: 58px
 background: -webkit-linear-gradient(top, #696868, #2E2E2E)
 background: -moz-linear-gradient(top, #696868, #2E2E2E)

.footer-button
  height: 58px
  border: 1px solid red

.voteButton
 max-width: 20px
 float: left
 padding: 0 10px 0 0
