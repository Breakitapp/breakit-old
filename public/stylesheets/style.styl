body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

.stream
  max-width: 350px

#nav-top-line
  height: 18px
  background: -webkit-linear-gradient(top, #8f1b1b, #d03636)
  background: -moz-linear-gradient(top, #8f1b1b, #d03636)

#nav-bottom-line
  min-height: 58px
  background: -webkit-linear-gradient(top, #fff, #ededed)
  background: -moz-linear-gradient(top, #fff, #ededed)

#nav-logo
  margin-left: 60px
  max-height: 58px
  float: left

.navbutton
  height: 58px
  float: left
  border: 1px solid red